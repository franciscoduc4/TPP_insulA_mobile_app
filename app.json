{
  "expo": {
    "name": "insulA",
    "slug": "insulA",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/insula_logo_no_text.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/insula_logo_no_text.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/insula_logo_no_text.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.insulA",
      "proguard": true,
      "buildTypes": {
        "release": {
          "enableSplitAPKs": true 
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
